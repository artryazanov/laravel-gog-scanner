{
  "name": "artryazanov/laravel-gog-scanner",
  "description": "Laravel 12 package to scan GOG.com games using queues and a fully normalized DB schema.",
  "type": "library",
  "license": "Unlicense",
  "authors": [
    { "name": "Artem Ryazanov", "email": "artryazanov@gmail.com" }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/http": "^11.0|^12.0",
    "illuminate/queue": "^11.0|^12.0",
    "illuminate/database": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Artryazanov\\GogScanner\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Artryazanov\\GogScanner\\GogScannerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^10.5 || ^11.0",
    "laravel/pint": "^1.24"
  },
  "autoload-dev": {
    "psr-4": {
      "Artryazanov\\GogScanner\\Tests\\": "tests/"
    }
  }
}
